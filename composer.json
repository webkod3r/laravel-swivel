{
    "name": "webkod3r/laravel-swivel",
    "type": "library",
    "description": "Laravel wrapper for swivel component",
    "keywords": [
        "laravel",
        "lumen",
        "swivel",
        "features",
        "feature-flags",
        "behavior",
        "component"
    ],
    "homepage": "https://github.com/webkod3r/laravel-swivel",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo Molina",
            "email": "web.kod3r@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "zumba/swivel": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "orchestra/database": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelSwivel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSwivel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "post-autoload-dump": [
            "bash ./extras/setup.sh"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "LaravelSwivel\\LaravelSwivelServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev"
}
